        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebScrapper"
        mc:Ignorable="d" Loaded="Window_Loaded"
        Title="DataGridTest" Height="1000" Width="1200" WindowState="Maximized">
    <Grid>
        <DataGrid x:Name="AllBooks" Margin="0,0,0,605"/>
        <DataGrid x:Name="SelectedBooks" Margin="0,492,0,0"/>
        <Button x:Name="btnMoveDown" Content="Down" HorizontalAlignment="Left" Margin="532,419,0,0" VerticalAlignment="Top" Height="50" Click="btnMoveDown_Click"/>
        <Button x:Name="btnMoveUp" Content="Up" HorizontalAlignment="Left" Margin="670,419,0,0" VerticalAlignment="Top" Height="50"/>

    </Grid>
</Window>
